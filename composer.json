{
  "name": "fond-of-spryker/brand",
  "description": "Brand module.",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    },
    {
      "name": "Felix Pfister",
      "email": "mail@felix-pfister.de"
    }
  ],
  "require": {
    "php": ">=7.1",
    "fond-of-spryker/brand-extension": "dev-master",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/config": "^3.0",
    "codeception/codeception": "^2.4",
    "php-coveralls/php-coveralls": "^2.1",
    "phpstan/phpstan": "^0.9",
    "sebastian/phpcpd": "^4.0",
    "spryker/code-sniffer": "^0.12",
    "fond-of-codeception/spryker": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
